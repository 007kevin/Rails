<% provide(:title, 'All users') %>
<h1>All users</h1>

<%= will_paginate %>

<ul class="users">
    <!-- In this context, Rails automatically looks for a partial called _user.html.erb
         We can call render directly on the @user variable since Rails infers @users
         is a list of User objects. -->
    <%= render @users %>
</ul>    

<%= will_paginate %>

